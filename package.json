{
  "name": "insyd-agency",
  "version": "0.0.1-development",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite dev",
    "prebuild": "npm run datasources && npm run sitemap",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:unit": "vitest",
    "lint": "prettier --check . && eslint .",
    "validate": "npm run check && npm run lint",
    "format": "prettier --write .",
    "datasources": "svelte-kit sync && tsx ./scripts/datasources.ts",
    "sitemap": "svelte-kit sync && tsx ./scripts/sitemap.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^3.1.0",
    "@sveltejs/kit": "^1.30.4",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/matter-js": "^0.18.5",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^1.2.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prismjs": "^1.29.0",
    "svelte": "^4.2.19",
    "svelte-check": "^4.1.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.4.16",
    "tslib": "^2.8.1",
    "tsx": "^4.20.5",
    "typescript": "^5.7.2",
    "vite": "^4.5.5",
    "vite-plugin-mkcert": "^1.17.6",
    "vitest": "^0.33.0"
  },
  "type": "module",
  "dependencies": {
    "@melt-ui/svelte": "^0.86.2",
    "@significa/svelte-ui": "^0.0.34",
    "@storyblok/js": "^3.1.9",
    "@tanstack/svelte-query": "^4.43.0",
    "matter-js": "^0.19.0",
    "prism-svelte": "^0.5.0",
    "resend": "^6.9.1",
    "svelte-confetti": "^1.4.0"
  }
}
